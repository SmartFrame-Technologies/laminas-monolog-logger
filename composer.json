{
  "name": "smartframe-technologies/laminas-monolog-logger",
  "description": "Implementation monolog library using laminas service manager and other libs",
  "minimum-stability": "stable",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Tomasz Wo≈∫niak",
      "email": "tomasz.wozniak@smartframe.io"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "monolog/monolog": "^2.1",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "laminas/laminas-diactoros": "^2.5",
    "psr/container": "^1.0",
    "psr/http-factory": "^1.0",
    "laminas/laminas-servicemanager": "^3.5",
    "elasticsearch/elasticsearch": "^7.10"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "SmartFrame\\Logger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SmartFrameTest\\Logger\\": "test/"
    }
  }
}
